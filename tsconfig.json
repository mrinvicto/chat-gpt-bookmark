{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "types": ["chrome"],
      "lib": ["dom", "esnext"],
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
    },
    "include": ["src"]
}
  